{
    "name": "vider/quiz-submissions-mu-plugins",
    "description": "MU Plugins for Quiz Submissions Management",
    "type": "wordpress-muplugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Vider Team",
            "email": "dev@vider.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.0 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Vider\\QuizSubmissions\\": "quiz-submissions/src/",
            "Vider\\SubmissionsViewer\\": "submissions-viewer/src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
